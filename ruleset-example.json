{
    "route": {
        "rules": [
            {
                "domain_keyword": [
                    "x.com",
                    "twimg.com",
                    "twitter.com",
                    "instagram.com",
                    "cdninstagram.com",
                    "medium.com",
                    "speedtest.net"
                ],
                "outbound": "vless-out"
            },
            {
                "rule_set": "geosite-youtube",
                "outbound": "vless-out"
            },
            {
                "protocol": "dns",
                "outbound": "dns-out"
            }
        ],
        "rule_set": [
            {
                "type": "local",
                "tag": "geosite-youtube",
                "format": "binary",
                "path": "./geosite-youtube.srs"
            }
        ],
        "auto_detect_interface": true
    }
}
