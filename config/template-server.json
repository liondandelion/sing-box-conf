{
    "log": {
        "output": "sing-box.log",
        "level": "error",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "vless",
            "tag": "vless-in",
            "listen": "::",
            "listen_port": 443,
            "users": [
                {
                    "name": "",
                    "uuid": "",
                    "flow": "xtls-rprx-vision"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "yahoo.com",
                "reality": {
                    "enabled": true,
                    "handshake": {
                        "server": "yahoo.com",
                        "server_port": 443
                    },
                    "private_key": "",
                    "short_id": [
                        ""
                    ],
                    "max_time_difference": "1m"
                }
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct-out"
        }
    ],
    "route": {
        "rules": [
			{
				"inbound": "tun-in",
				"action": "sniff"
			},
            {
                "protocol": "bittorrent",
                "action": "reject"
            }
        ]
    }
}
