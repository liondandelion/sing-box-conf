{
  "log": {
    "level": "error",
    "output": "sing-box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-local",
        "address": "77.88.8.8",
        "detour": "direct-out"
      },
      {
        "tag": "dns-proxy",
        "address": "https://1.1.1.1/dns-query",
        "detour": "vless-out"
      }
    ],
    "rules": [
      {
        "rule_set": [
          "geosite-anvyko",
          "local-rules"
        ],
        "server": "dns-proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "address": "10.11.12.1/24",
      "auto_route": true,
      "stack": "system",
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "vless",
      "tag": "vless-out",
      "server": "",
      "server_port": 443,
      "uuid": "",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true,
        "server_name": "yahoo.com",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true,
          "public_key": "",
          "short_id": ""
        }
      }
    }
  ],
  "route": {
    "rules": [
      {
        "rule_set": [
          "geosite-anvyko",
          "local-rules"
        ],
        "outbound": "vless-out"
      },
      {
        "protocol": "dns",
        "outbound": "dns-out"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-anvyko",
        "format": "binary",
        "url": "https://github.com/liondandelion/sing-box-conf/releases/latest/download/geosite-anvyko.srs",
        "download_detour": "direct-out",
        "update_interval": "1h0m0s"
      },
      {
        "type": "local",
        "tag": "local-rules",
        "format": "source",
        "path": "local-rules.json"
      }
    ],
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  }
}
