{
  "log": {
    "level": "error",
    "output": "sing-box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "type": "tcp",
        "tag": "dns-direct",
        "server": "77.88.8.8"
      },
      {
        "type": "tcp",
        "tag": "dns-proxy",
        "detour": "vless-out",
        "server": "8.8.8.8"
      }
    ],
    "rules": [
      {
        "rule_set": [
          "geosite-anvyko",
          "local-rules"
        ],
        "server": "dns-proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "mtu": 1452,
      "address": "10.11.12.1/24",
      "auto_route": true,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out",
      "udp_fragment": true
    },
    {
      "type": "vless",
      "tag": "vless-out",
      "udp_fragment": true,
      "server": "",
      "server_port": 443,
      "uuid": "",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true,
        "server_name": "yahoo.com",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true
        }
      }
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "tun-in",
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "rule_set": [
          "geosite-anvyko",
          "local-rules"
        ],
        "outbound": "vless-out"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-anvyko",
        "url": "https://github.com/liondandelion/sing-box-conf/releases/latest/download/geosite-anvyko.srs",
        "download_detour": "direct-out",
        "update_interval": "1h0m0s"
      },
      {
        "tag": "local-rules",
        "rules": [
          {
            "domain": "example.com",
            "domain_suffix": ".example.com"
          }
        ]
      }
    ],
    "auto_detect_interface": true,
    "default_domain_resolver": "dns-direct"
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  }
}
